{
    "version": 2,
    "name": "puzzle-master-server",
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app.js"
      },
      {
        "src": "/",
        "dest": "/app.js"
      }
    ]
  }